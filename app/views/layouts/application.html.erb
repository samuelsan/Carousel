<!DOCTYPE html>
<html>
<head>
  <title>Carousel</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>

<div id="user-widget">
  <% if (current_user) %>
    Welcome <strong><%= current_user.name %></strong>!
    <%= link_to "Sign out", signout_path, id: "sign_out" %>
  <% else %>
    <%= link_to "Sign in with Facebook", "/auth/facebook", id: "sign_in" %>
  <% end %>
</div>

<%= yield %>
<script src="/javascripts/phaser.js"></script>
<script src="/javascripts/box2d-plugin-full.js" type="text/javascript"></script>
<script src="/javascripts/modules/factories/firefly.js"></script>
<script src="/javascripts/modules/factories/projectile.js"></script>
<script src="/javascripts/modules/states/Boot.js"></script>
<script src="/javascripts/modules/states/Preload.js"></script>
<script src="/javascripts/modules/states/Oaktree.js"></script>
<script src="/javascripts/modules/states/Stream.js"></script>
<script src="/javascripts/modules/states/Minimenu.js"></script>
<script src="/javascripts/modules/states/Minigame.js"></script>
<script src="/javascripts/modules/states/EndDemo.js"></script>
<script src="/javascripts/modules/CarouselGame.js"></script>
<script src="/javascripts/modules/factories/fish.js"></script>
</body>
</html>
